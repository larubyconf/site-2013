%tr
  %td
    =photo.caption
  %td
    =photo.sort_index
  %td
    =image_tag(photo.image.url(:small))
  %td
    =photo.created_at
  %td.center
    =link_to edit_admin_photo_path(photo) do
      %i.icon-pencil
    =link_to admin_photo_path(photo), :method => :delete, :rel => 'nofollow' do
      %i.icon-trash