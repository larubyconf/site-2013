%tr
  %td
    = link_to user.email, admin_user_path(user)
  %td
    =user.authentications.count
  %td
    =user.sign_in_count
  %td
    =time_ago_in_words(user.current_sign_in_at)
    ago
  %td.center
    =link_to edit_admin_user_path(user) do
      %i.icon-pencil
    =link_to admin_user_path(user), :method => :delete, :rel => 'nofollow' do
      %i.icon-trash
